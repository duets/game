{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "root.json",
  "title": "Database",
  "description": "Root of the database",
  "type": "object",
  "required": ["countries", "genres", "instruments", "skills", "objects"],
  "properties": {
    "countries": { "$ref": "file.json#definitions/file" },
    "genres": { "$ref": "file.json#definitions/file" },
    "instruments": { "$ref": "file.json#definitions/file" },
    "skills": { "$ref": "file.json#definitions/file" },
    "objects": { "$ref": "file.json#definitions/file" }
  }
}
